{
  "manifest_version": 2,

  "name": "Local Mapper",
  "description": "This extension will Replace the Local File.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "<all_urls>"
 ],

  "browser_action": {
    "default_icon": "icon_off.png",
    "default_popup": "mapper_form.html"
  }
}
